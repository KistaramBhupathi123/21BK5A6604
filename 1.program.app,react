import React from 'react';
import { BrowserRouter as Router, Route, Switch } from 'react-router-dom';
import TrainSchedule from './components/TrainSchedule';

function App() {
  return (
    <Router>
      <Switch>
        <Route exact path="/" component={TrainSchedule} />
      </Switch>
    </Router>
  );
}

export default App;
